{
  "name": "Kommentarlos",
  "version": "0.1",
  "description": "Entferne Kommentare von Nachrichtenwebseiten",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://*.derstandard.at/*"],
      "css": ["css/derstandardat.css"]
    },
    {
      "matches": ["https://*.kleinezeitung.at/*"],
      "css": ["css/kleinezeitungat.css"]
    },
    {
      "matches": ["https://*.krone.at/*"],
      "css": ["css/kroneat.css"]
    },
    {
      "matches": ["https://*.oe24.at/*"],
      "css": ["css/oe24at.css"]
    },
    {
      "matches": ["https://*.nachrichten.at/*"],
      "css": ["css/nachrichtenat.css"]
    }
  ]
}
